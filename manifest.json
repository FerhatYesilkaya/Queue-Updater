{
  "manifest_version": 3,
  "name": "Queue-Updater",
  "version": "1.1",
  "permissions": ["activeTab", "scripting", "storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/Roche_Logo.png"
    }
  },
  "host_permissions": [
    "https://rexis.lightning.force.com/*"
  ]
}
